<h1>Create a Song</h1>
<ul>
<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br />
  <%= f.label :artist %>
  <%= f.text_field :artist_name %>
  <br>
  <%= f.label :genre_id %>
  <%= f.collection_select :genre_id, @genres, :id, :name %>
  <br>
  <%= f.label :song_notes %>
    <input name="song[note_contents][]", id="song_notes_1"><br>
    <input name="song[note_contents][]", id="song_notes_2"><br>
    <input name="song[note_contents][]", id="song_notes_3"><br>
    <br>
  <%= f.submit %>
<% end %>
</ul>
